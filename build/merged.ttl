@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ns1: <https://ben.example/ns/pim#> .
@prefix ns2: <http://www.w3.org/2002/12/cal/ical#> .
@prefix ns3: <https://ben.example/pim/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns3:bookmark-001 a schema:BookmarkAction ;
    dcterms:description "SPARQL server documentation" ;
    dcterms:title "Apache Jena Fuseki" ;
    ns3:hasTag ns3:tag-knowledge ;
    schema:target <https://jena.apache.org/documentation/fuseki2/> .

ns3:contact-001 a foaf:Person ;
    rdfs:label "Jane Doe" ;
    foaf:mbox <mailto:jane@example.com> ;
    ns3:hasTag ns3:tag-knowledge .

ns3:event-001 a ns2:Vevent ;
    dcterms:title "RDF Workshop" ;
    ns2:dtstart "2025-09-15T09:00:00+00:00"^^xsd:dateTime ;
    ns2:location "London" ;
    ns3:hasTag ns3:tag-knowledge .

ns3:note-001 a schema:CreativeWork ;
    dcterms:created "2025-08-18T14:00:00+00:00"^^xsd:dateTime ;
    dcterms:creator ns3:Ben ;
    dcterms:title "First RDF note" ;
    ns3:hasTag ns3:tag-knowledge ;
    schema:text "This is a test note in the RDF knowledge base." .

ns3:note-test a schema:CreativeWork ;
    dcterms:created "2025-08-18T15:00:00+00:00"^^xsd:dateTime ;
    dcterms:title "Test Note" ;
    ns3:hasTag ns3:tag-test ;
    schema:text "This is a test note for validation" .

ns3:project-001 a ns1:Project ;
    dcterms:description "A personal knowledge management system using RDF and SPARQL" ;
    dcterms:title "RDF Personal Knowledge Base" .

ns3:task-001 a ns1:Task ;
    dcterms:created "2025-08-18T14:30:00+00:00"^^xsd:dateTime ;
    dcterms:title "Finish RDF PIM design" ;
    ns1:assignedTo ns3:Ben ;
    ns1:priority 2 ;
    ns1:status "todo" .

ns3:TagScheme a skos:ConceptScheme ;
    dcterms:description "Tags for organizing personal knowledge" ;
    dcterms:title "Personal Tags" .

ns3:Ben a ns3:Person ;
    rdfs:label "Ben Mordue" ;
    ns3:email "ben@example.com" .

ns3:tag-knowledge a skos:Concept ;
    skos:definition "Items related to knowledge management and learning" ;
    skos:inScheme ns3:TagScheme ;
    skos:prefLabel "Knowledge" .

