PREFIX : <https://ben.example/pim/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

:Ben    rdf:type    :Person;
        rdfs:label  "Ben Mordue";
        :email      "ben@example.com" .

PREFIX : <https://ben.example/pim/>

PREFIX pim: <https://ben.example/ns/pim#>

PREFIX dcterms: <http://purl.org/dc/terms/>

PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

:task-001  rdf:type      pim:Task;
        dcterms:title    "Finish RDF PIM design";
        dcterms:created  "2025-08-18T14:30:00Z"^^xsd:dateTime;
        pim:status       "todo";
        pim:priority     2;
        pim:assignedTo   :Ben .

PREFIX : <https://ben.example/pim/>

PREFIX schema: <https://schema.org/>

PREFIX dcterms: <http://purl.org/dc/terms/>

PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

:note-001  rdf:type      schema:CreativeWork;
        dcterms:title    "First RDF note";
        dcterms:creator  :Ben;
        dcterms:created  "2025-08-18T14:00:00Z"^^xsd:dateTime;
        schema:text      "This is a test note in the RDF knowledge base.";
        :hasTag          :tag-knowledge .

PREFIX : <https://ben.example/pim/>

PREFIX foaf: <http://xmlns.com/foaf/0.1/>

PREFIX vcard: <http://www.w3.org/2006/vcard/ns#>

PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

:contact-001  rdf:type  foaf:Person;
        rdfs:label  "Jane Doe";
        foaf:mbox   <mailto:jane@example.com>;
        :hasTag     :tag-knowledge .

PREFIX : <https://ben.example/pim/>

PREFIX pim: <https://ben.example/ns/pim#>

PREFIX dcterms: <http://purl.org/dc/terms/>

PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

:project-001  rdf:type       pim:Project;
        dcterms:title        "RDF Personal Knowledge Base";
        dcterms:description  "A personal knowledge management system using RDF and SPARQL" .

PREFIX : <https://ben.example/pim/>

PREFIX schema: <https://schema.org/>

PREFIX dcterms: <http://purl.org/dc/terms/>

:bookmark-001  rdf:type      schema:BookmarkAction;
        dcterms:title        "Apache Jena Fuseki";
        schema:target        <https://jena.apache.org/documentation/fuseki2/>;
        dcterms:description  "SPARQL server documentation";
        :hasTag              :tag-knowledge .

PREFIX : <https://ben.example/pim/>

PREFIX ical: <http://www.w3.org/2002/12/cal/ical#>

PREFIX dcterms: <http://purl.org/dc/terms/>

PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

:event-001  rdf:type   ical:Vevent;
        dcterms:title  "RDF Workshop";
        ical:dtstart   "2025-09-15T09:00:00Z"^^xsd:dateTime;
        ical:location  "London";
        :hasTag        :tag-knowledge .
PREFIX : <https://ben.example/pim/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

:TagScheme  rdf:type         skos:ConceptScheme;
        dcterms:title        "Personal Tags";
        dcterms:description  "Tags for organizing personal knowledge" .

:tag-knowledge  rdf:type  skos:Concept;
        skos:inScheme    :TagScheme;
        skos:prefLabel   "Knowledge";
        skos:definition  "Items related to knowledge management and learning" .
